<!DOCTYPE html>
<html lang="pt-BR">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>API Dados Receita Federal CNPJ</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>

<body>
    <div class="content">
        {{#each empresa}}
        <table class="table-principal border">
            <tbody>
                <tr>
                    <td>
                        <table id="tb-header">
                            <tr id="tr-tb-header">
                                <td> <img id="brasao" src="/images/brasao2.gif" alt="Brasão"> </td>
                                <td>
                                    <p class="title-header">REPÚBLICA FEDERATIVA DO BRASIL</p>
                                    <br>
                                    <p class="title-header">CADASTRO NACIONAL DA PESSOA JURÍDICA</p>
                                </td>
                                <td>
                                    <span>{{this.id}}</span>
                                </td>
                            </tr>

                        </table>
                        <table class="border" id="tb-inscricao">
                            <tr>
                                <td>
                                    <p class="title">NÚMERO DE INSCRIÇÃO</p>
                                    <p>{{this.cnpj}}</p>
                                    <p>{{this.matriz_filial}}</p>
                                </td>
                                <td id="title-inscricao" class="border-left">COMPROVANTE DE INSCRIÇÃO E DE
                                    SITUAÇÃO
                                    CADASTRAL</td>
                                <td class="border-left">
                                    <p class="title">DATA DE ABERTURA</p>
                                    <p>{{this.data_inicio_ativ}}</p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-nome-empresarial">
                            <tr>
                                <td class="border">
                                    <p class="title">NOME EMPRESARIAL</p>
                                    <p>{{this.razao_social}}</p>

                                </td>
                            </tr>
                        </table>
                        <table id="tb-nome-fantasia">
                            <tr>
                                <td class="border">
                                    <p class="title">TÍTULO DO ESTABELECIMENTO (NOME DE FANTASIA)</p>
                                    <p>{{this.nome_fantasia}}</p>
                                </td>
                                <td class="border">
                                    <p class="title">PORTE</p>
                                    <p>{{this.porte}}</p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-atividade-principal">
                            <tr>
                                <td class="border">
                                    <p class="title">CÓDIGO E DESCRIÇÃO DA ATIVIDADE ECONÔMICA PRINCIPAL</p>
                                    <p>{{this.cnae_fiscal}} - {{this.descricao_cnae_fiscal}}</p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-atividade-secundaria">
                            <tr>
                                <td class="border">
                                    <p class="title">CÓDIGO E DESCRIÇÃO DAS ATIVIDADES ECONÔMICAS SECUNDÁRIAS</p>
                                    {{#each this.cnaes}}
                                    <p>{{this.cnae}} - {{this.descricao_cnae}}</p>
                                    {{/each}}
                                </td>
                            </tr>
                        </table>
                        <table id="tb-natureza-juridica">
                            <tr>
                                <td class="border">
                                    <p class="title">CÓDIGO E DESCRIÇÃO DA NATUREZA JURÍDICA</p>
                                    <p>{{this.cod_nat_juridica}}</p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-logradouro">
                            <tr>
                                <td class="border">
                                    <p class="title">LOGRADOURO</p>
                                    <p>{{this.logradouro}}</p>
                                </td>
                                <td class="border">
                                    <p class="title">NÚMERO</p>
                                    <p>{{this.numero}}</p>
                                </td>
                                <td class="border complemento">
                                    <p class="title">COMPLEMENTO</p>
                                    <p>{{this.complemento}}</p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-logradouro2">
                            <tr>
                                <td class="border">
                                    <p class="title">CEP</p>
                                    <p>{{this.cep}}</p>
                                </td>
                                <td class="border">
                                    <p class="title">BAIRRO/DISTRITO</p>
                                    <p>{{this.bairro}}</p>
                                </td>
                                <td class="border">
                                    <p class="title">MUNICÍPIO</p>
                                    <p>{{this.municipio}}</p>
                                </td>
                                <td class="border">
                                    <p class="title">UF</p>
                                    <p>{{this.uf}}</p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-contato">
                            <tr>
                                <td class="border">
                                    <p class="title">ENDEREÇO ELETRÔNICO</p>
                                    <p>{{this.email}}</p>
                                </td>
                                <td class="border">
                                    <p class="title">TELEFONE</p>
                                    <p>({{this.ddd_1}}) {{this.telefone_1}} 
                                    {{#if this.telefone_2}}
                                     / ({{this.ddd_2}}) {{this.telefone_2}}
                                    {{/if}}
                                     {{#if this.num_fax}}
                                     / ({{this.ddd_fax}}) {{this.num_fax}}
                                    {{/if}} 
                                    </p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-efr">
                            <tr>
                                <td class="border">
                                    <p class="title">ENTE FEDERATIVO RESPONSÁVEL (EFR)</p>
                                    <p><br/></p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-situacao-cadastral">
                            <tr>
                                <td class="border">
                                    <p class="title">SITUAÇÃO CADASTRAL</p>
                                    <p>{{this.situacao}}</p>
                                </td>
                                <td class="border">
                                    <p class="title">DATA DA SITUAÇÃO CADASTRAL</p>
                                    <p>{{this.data_situacao}}</p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-motivo-situacao-cadastral">
                            <tr>
                                <td class="border">
                                    <p class="title">MOTIVO DE SITUAÇÃO CADASTRAL</p>
                                    <p>{{this.motivo_situacao}}</p>
                                </td>
                            </tr>
                        </table>
                        <table id="tb-situacao-especial">
                            <tr>
                                <td class="border">
                                    <p class="title">SITUAÇÃO ESPECIAL</p>
                                    {{#if this.sit_especial}}
                                    <p>{{this.sit_especial}}</p>
                                    {{else}}
                                    <p>********</p>
                                    {{/if}}
                                </td>
                                <td class="border">
                                    <p class="title">DATA DA SITUAÇÃO ESPECIAL</p>
                                    {{#if data_sit_especial}}
                                    <p>{{data_sit_especial}}</p>
                                    {{else}}
                                    <p>********</p>
                                    {{/if}}
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>

            </tbody>

        </table>
        {{/each}}
    </div>
</body>

</html>